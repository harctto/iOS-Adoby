	<array>
		<string>Prompt-Light.ttf</string>
		<string>Prompt-Medium.ttf</string>
		<string>Prompt-Regular.ttf</string>
		<string>Prompt-SemiBold.ttf</string>
		<string>Prompt-Thin.ttf</string>
		<string>Prompt-Bold.ttf</string>
